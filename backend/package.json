{
  "name": "fitness-box",
  "version": "4.0.0",
  "description": "FitnessBox backend",
  "keywords": [
    "rest",
    "api",
    "node.js"
  ],
  "main": "main.js",
  "scripts": {
    "start": "npm run build && node ./dist/main.rest.js",
    "start:dev": "npx tsx ./src/main.rest.ts | pino-pretty --colorize --translateTime SYS:standard",
    "build": "npm run clean && npm run compile",
    "lint": "eslint src/ --ext .ts",
    "compile": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "ts": "ts-node",
    "mock:server": "json-server ./mocks/mock-server-data.json --port 3123"
  },
  "devDependencies": {
    "@types/node": "18.16.3",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.46.0",
    "eslint": "8.39.0",
    "eslint-config-htmlacademy": "9.1.1",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "tsx": "^4.19.3",
    "typescript": "4.9.5"
  },
  "type": "module",
  "engines": {
    "node": "^18.0.0",
    "npm": ">=8"
  },
  "dependencies": {
    "@types/convict": "^6.1.6",
    "@types/convict-format-with-validator": "^6.0.5",
    "convict": "^6.2.4",
    "convict-format-with-validator": "^6.2.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "got": "^14.4.6",
    "inversify": "^7.3.0",
    "json-server": "^1.0.0-beta.3",
    "pino": "^9.6.0",
    "reflect-metadata": "^0.2.2",
    "tslib": "^2.8.1"
  }
}
